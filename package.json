{
  "name": "foo-bar-baz-qux",
  "version": "0.0.0-development",
  "description": "Some foo bar baz, playing with automation of semantic-release",
  "main": "index.js",
  "scripts": {
    "deps": "dependency-check .",
    "secure": "nsp check",
    "lint": "eslint --fix ./*.js",
    "test": "nyc --reporter lcov node test.js",
    "report": "nyc report",
    "git": "npm-run-all --silent -s git:*",
    "git:add": "git add --all",
    "git:cz": "simple-commit-message",
    "git:push": "git push",
    "commit": "npm-run-all --silent -s deps secure lint test git",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "prettier-eslint": "6.4.2"
  },
  "devDependencies": {
    "dependency-check": "2.9.1",
    "eslint": "4.3.0",
    "mukla": "0.4.9",
    "npm-run-all": "4.0.2",
    "nsp": "2.7.0",
    "nyc": "11.0.3",
    "pre-commit": "1.2.2",
    "semantic-release": "6.3.6",
    "simple-commit-message": "3.3.1",
    "standard": "9.0.2"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "nyc": {
    "check-coverage": true,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "lines": 0
  },
  "release": {
    "analyzeCommits": "simple-commit-message"
  },
  "pre-commit": {
    "silent": true,
    "run": [
      "deps",
      "secure",
      "lint",
      "test"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tunnckoCore/foo-bar-baz-qux.git"
  }
}
